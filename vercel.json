{
      "builds": [
        {
          "src": "package.json",
          "use": "@vercel/node",
          "config": {
            "distDir": "dist"
          }
        }
      ],
      "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/index.html"
        }
      ],
      "redirects": [
        {
          "source": "/Timothy-Cao.github.io",
          "destination": "https://timcao.com",
          "permanent": true
        },
        {
          "source": "/Timothy-Cao.github.io/(.*)",
          "destination": "https://timcao.com/$1",
          "permanent": true
        }
      ]
    }