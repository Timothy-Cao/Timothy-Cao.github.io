{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "redirects": [
    {
      "source": "/Timothy-Cao.github.io/(.*)",
      "destination": "/$1",
      "permanent": true
    }
  ]
}